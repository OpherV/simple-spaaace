{
  "name": "lance-game",
  "version": "1.0.0",
  "engines": {
    "node": "4.3.0"
  },
  "description": "An HTML5 multiplayer game based on Lance",
  "main": "main.js",
  "dependencies": {
    "browserify": "^13.0.1",
